{
  "name": "@maple-plan/web-e2e",
  "version": "0.0.1",
  "private": true,
  "nx": {
    "implicitDependencies": [
      "@maple-plan/web"
    ]
    ,
    "targets": {
      "e2e": {
        "executor": "@nx/cypress:cypress",
        "options": {
          "cypressConfig": "apps/web-e2e/cypress.config.cjs",
          "testingType": "e2e",
          "env": {
            "TS_NODE_COMPILER_OPTIONS": "{\"moduleResolution\":\"node16\"}"
          }
        },
        "configurations": {
          "ci": {
            "headless": true
          }
        }
      }
    }
  }
}
