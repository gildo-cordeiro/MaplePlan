{
  "name": "@maple-plan/web",
  "version": "0.0.1",
  "private": true,
  "dependencies": {
    "next": "~15.2.4",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  }

  ,"nx": {
    "targets": {
      "serve": {
        "executor": "nx:run-commands",
        "options": {
          "command": "next dev",
          "cwd": "apps/web"
        },
        "configurations": {
          "production": {
            "command": "next start",
            "cwd": "apps/web"
          }
        }
      },
      "start": {
        "executor": "nx:run-commands",
        "options": {
          "command": "next start",
          "cwd": "apps/web"
        }
      }
    }
  }
}
