{
  "name": "@maple-plan/web",
  "version": "0.0.1",
  "private": true,
  "dependencies": {
    "next": "~15.2.4",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  }

  ,"nx": {
    "targets": {
      "build": {
        "executor": "@nx/next:build",
        "options": {
          "outputPath": "dist/apps/web"
        },
        "configurations": {
          "production": {}
        }
      },
      "serve": {
        "executor": "@nx/next:server",
        "options": {
          "buildTarget": "@maple-plan/web:build",
          "dev": true,
          "port": 3000
        },
        "configurations": {
          "production": {
            "dev": false
          }
        }
      },
      "start": {
        "executor": "@nx/next:server",
        "options": {
          "buildTarget": "@maple-plan/web:build",
          "dev": false,
          "port": 3000
        }
      }
    }
  }
}
